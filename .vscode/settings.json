{
    "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true,
      "*.aux": true
    },
    "editor.rulers": [
      60, 80, 100
    ],
    "editor.renderWhitespace": "boundary",
    "files.trimTrailingWhitespace": true,
    "editor.minimap.enabled": true,
    "editor.minimap.renderCharacters": false,
    "git.autofetch": false,
    "workbench.editor.showTabs": false,
    "editor.formatOnSave": false,
    "files.autoSave": "onFocusChange",
    "cSpell.words": [
      "Bmatrix",
      "Enter",
      "Image",
      "Local",
      "abstract",
      "alpha",
      "appendix",
      "backmatter",
      "cases",
      "cite",
      "clearpage",
      "definition",
      "floatstyle",
      "include",
      "includegraphics",
      "integral",
      "latex",
      "listings",
      "macros",
      "mainmatter",
      "matplotlib",
      "mgrid",
      "mode",
      "newtheorem",
      "numpy",
      "preamble",
      "pyplot",
      "russian",
      "streamplot",
      "theorem",
      "verbatim",
      "Спиралевидно"
    ]
  }